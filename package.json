{
  "name": "ucare-mental-health-chatbot",
  "version": "1.0.0",
  "description": "AI-powered mental health chatbot with Google Gemini integration",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm start",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "postinstall": "cd frontend && npm install && npm run build && cd ../backend && npm install"
  },
  "keywords": [
    "chatbot",
    "mental-health",
    "ai",
    "gemini",
    "react",
    "nodejs",
    "express"
  ],
  "author": "umesh-blip",
  "license": "MIT",
  "engines": {
    "node": "20.x",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umesh-blip/ucare-mental-health-chatbot.git"
  },
  "bugs": {
    "url": "https://github.com/umesh-blip/ucare-mental-health-chatbot/issues"
  },
  "homepage": "https://github.com/umesh-blip/ucare-mental-health-chatbot#readme"
}
